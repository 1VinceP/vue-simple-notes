   },
   "devDependencies": {
      "@vue/cli-plugin-babel": "^3.0.3",
      "@vue/cli-plugin-eslint": "^3.0.3",
      "@vue/cli-plugin-unit-jest": "^4.5.6",
      "@vue/cli-service": "^3.0.3",
      "@vue/test-utils": "^1.0.3",
      "babel-eslint": "^10.0.1",
      "eslint": "^5.16.0",
      "eslint-plugin-vue": "^5.0.0",
      "husky": "^4.3.0",
      "lint-staged": "^10.4.0",
      "node-sass": "^4.12.0",
      "prettier": "^2.1.2",
      "sass-loader": "^8.0.0",
      "vue-template-compiler": "^2.6.10"
   },
   "lint-staged": {
      "*.vue": [
         "eslint"
